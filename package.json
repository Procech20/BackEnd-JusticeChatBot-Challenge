{
  "name": "nodejs-stack-app",
  "version": "1.0.0",
  "description": "A progressive nodejs express api for backend skills development and learning testing.",
  "main": "server.js",
  "scripts": {
    "test": "",
    "build": "babel ./src --out-dir build",
    "dev": "nodemon --exec babel-node src/server",
    "start": "npm run seed && npm run build && node build",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "npm run migrate:undo && npm run migrate && npx sequelize db:seed:undo:all && npx sequelize db:seed:all",
    "seed:test": "cross-env NODE_ENV=test npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/Procech20/BackEnd-JusticeChatBot-Challenge"
  },
  "keywords": [
    "Justice",
    "ChatBot"
  ],
  "author": "Procech",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.13.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "crossenv": "0.0.2-security",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.22.1",
    "jshint": "^2.12.0",
    "nodemon": "^2.0.7"
  }
}
